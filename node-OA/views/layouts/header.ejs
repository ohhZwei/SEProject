                <% let userType =['普通用户','发布单位','SuperSU']%>
                <% if(parseInt(userinfo.status)!=1){%>
                <li><a id="userinfo-username">您是 <%=userType[parseInt(userinfo.status)]%></a></li>

                <%} else{%>

                <li><a id="userinfo-department"><%=userinfo.department_name%></a></li>
                <%}%>

                <%}%>
                <li><a href="/routes/login/logout">注销</a>
                </li>
            </ul>
        </div>
    </div>
